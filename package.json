{
  "name": "bn2",
  "version": "1.1.0",
  "description": "A Bignumber.js Wrapper",
  "main": "cjs/index.js",
  "module": "mjs/index.js",
  "scripts": {
    "build-mjs": "node_modules/babel-cli/bin/babel.js src --out-dir mjs",
    "build-cjs": "node_modules/babel-cli/bin/babel.js src --out-dir cjs --presets env,stage-0 --no-babelrc",
    "build": "npm run build-mjs && npm run build-cjs"
  },
  "dependencies": {
    "bignumber.js": "^7.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:cutsin/bn2.git"
  },
  "license": "MIT"
}
